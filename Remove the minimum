https://www.codewars.com/kata/563cf89eb4747c5fb100001b

function removeSmallest(num) {
  let arr = [];
  for(let i = 0; i < num.length; i++) {
    arr.push(num[i]);
  }
  let min = arr[0],
      minIndex = 0;
    for (let j = 1; j < arr.length; j++) {
      if(arr[j] < min) {
      min = arr[j];
      minIndex = j;
      }
    }
     arr.splice(minIndex, 1);
     return arr;
}
