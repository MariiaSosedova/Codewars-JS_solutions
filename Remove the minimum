https://www.codewars.com/kata/563cf89eb4747c5fb100001b

function removeSmallest(num) {
  let arr = num.slice(0);
  let min = arr[0],
      minIndex = 0;
    for (let i = 1; i < arr.length; i++) {
      if(arr[i] < min) {
      min = arr[i];
      minIndex = i;
      }
    }
     arr.splice(minIndex, 1);
     return arr;
}
